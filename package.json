{
    "name": "yim-ui",
    "version": "1.0.87",
    "files": [
        "lib/**/*",
        "packages/**/*",
        "dist/**/*",
        "util/**/*"
    ],
    "type": "module",
    "repository": "https://github.com/wsf1992/ymi-ui.git",
    "homepage": "https://github.com/wsf1992/ymi-ui",
    "main": "lib/main.js",
    "exports": {
        ".": {
            "import": "./lib/main.js",
            "require": "./lib/main.cjs"
        },
        "./css": "./lib/style.css",
        "./utils": {
            "import": "./util/main.js",
            "require": "./util/main.cjs"
        }
    },
    "scripts": {
        "serve": "vite --config ./vite/vite.dev.config.js",
        "build:lib": "vite build --config ./vite/vite.lib.config.js",
        "build:app": "vite build --config ./vite/vite.app.config.js",
        "build:utils": "vite build --config ./vite/vite.utils.config.js",
        "preview": "vite preview"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.4",
        "@vitejs/plugin-vue2": "^2.2.0",
        "element-ui": "^2.15.14",
        "github-markdown-css": "^5.5.0",
        "prettier": "^3.1.1",
        "sass": "^1.49.0",
        "typescript": "^4.7.4",
        "unplugin-vue-components": "^0.26.0",
        "vite": "^4.4.9",
        "vite-plugin-md": "^0.21.5",
        "vue": "^2.7.0",
        "vue-router": "^3.6.5"
    },
    "description": "",
    "author": "",
    "license": "ISC"
}
